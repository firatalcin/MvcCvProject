
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<h4>Yeni Deneyim Ekleme Sayfası</h4>
<br />
<form class="form-group" method="post">
    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı giriniz" />
    <br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlığı giriniz" />
    <br />
    <input type="text" name="Tarih" class="form-control" placeholder="Çalışma Tarihini Giriniz" />
    <br />
    <textarea placeholder="Açıklama" name="Aciklama" class="form-control" rows="5"></textarea>
    <br />
    <button class="btn btn-info">Kaydet</button>
</form>

